  <body>
   <div class="nav">
    <div class="menu">
    <a href="/emoji/Sports?sort=happy">
      <label>Sports</label>
      <g-emoji class="g-emoji" alias="football" fallback-src="https://assets-cdn.github.com/images/icons/emoji/unicode/1f3c8.png">🏈</g-emoji>
    </a>  
    <a href="/emoji/Food?sort=happy">
      <label>Food</label>
      <g-emoji class="g-emoji" alias="pizza" fallback-src="https://assets-cdn.github.com/images/icons/emoji/unicode/1f355.png">🍕</g-emoji>
    </a>
    <a href="/emoji/Weather?sort=happy">
      <label>Weather</label>
      <g-emoji class="g-emoji" alias="umbrella" fallback-src="https://assets-cdn.github.com/images/icons/emoji/unicode/2614.png">☔️</g-emoji>
    </a>
    <a href="/emoji/Pet?sort=happy">
      <label>Pets</label>
      <g-emoji class="g-emoji" alias="feet" fallback-src="https://assets-cdn.github.com/images/icons/emoji/unicode/1f43e.png">🐾</g-emoji>

    </a>
    <a href="/emoji/Work?sort=happy">
      <label>Work</label>
    <g-emoji class="g-emoji" alias="office" fallback-src="https://assets-cdn.github.com/images/icons/emoji/unicode/1f3e2.png">🏢</g-emoji>
  </a>
 </div>
   <div class="user">
    {{#if user}}
     <a href="/profile"> {{user.username}}</a>
      
    <br>
    <a href="/logout">LogOut</a>
  
